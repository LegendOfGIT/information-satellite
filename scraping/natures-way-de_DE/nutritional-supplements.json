{
  "site": "naturesway.de",

  "scraping": [
    {
      "comment": "site",
      "commandId": "set-context-value",
      "parameters": {
        "contextId": "site",
        "value": "natures-way-de_DE"
      }
    },
    {
      "comment": "locale",
      "commandId": "set-context-value",
      "parameters": {
        "contextId": "locale",
        "value": "de-DE"
      }
    },
    {
      "comment": "usecase",
      "commandId": "set-context-value",
      "parameters": {
        "contextId": "usecase",
        "value": "single-item"
      }
    },
    {
      "comment": "navigation-path",
      "commandId": "set-context-value",
      "parameters": {
        "contextId": "navigation-path",
        "value": [
          "BEAUTY_CARE",
          "BEAUTY_CARE_HEALTH",
          "BEAUTY_CARE_HEALTH_NUTRITIALSUPPLEMENTS"
        ]
      }
    },

    {
      "comment": "product-overview",
      "commandId": "visit-uri",
      "parameters": {
        "contextId": "product-overview",
        "uri": "https://{template.site}/shop/"
      }
    },
    {
      "comment": "product-ids",
      "commandId": "get-values-by-css-queries",
      "parameters": {
        "contextId": "product-ids",
        "css-queries": [ ".woocommerce-loop-product__link"],
        "attribute-id": "href",
        "sourceContextId": "product-overview"
      }
    },
    {
      "comment": "product-ids",
      "commandId": "clone-items-and-modify-clones",
      "parameters": {
        "contextId": "product-ids",
        "replacements": {
          "\/$": "",
          ".*\/": ""
        },
        "sourceContextId": "product-ids"
      }
    },
    {
      "comment": "observe scraped information",
      "commandId": "observe-information",
      "parameters": {
        "informationIds": [
          "locale",
          "navigation-path",
          "product-ids",
          "site",
          "usecase"
        ]
      }
    }
  ]
}
