{
  "site": "songmics.de",

  "scraping": [
    {
      "comment": "site",
      "commandId": "set-context-value",
      "parameters": {
        "contextId": "site",
        "value": "songmics-de_DE"
      }
    },
    {
      "comment": "locale",
      "commandId": "set-context-value",
      "parameters": {
        "contextId": "locale",
        "value": "de-DE"
      }
    },
    {
      "comment": "usecase",
      "commandId": "set-context-value",
      "parameters": {
        "contextId": "usecase",
        "value": "single-item"
      }
    },
    {
      "comment": "navigation-path",
      "commandId": "set-context-value",
      "parameters": {
        "contextId": "navigation-path",
        "value": [
          "HOME",
          "HOME_FURNITURE",
          "HOME_FURNITURE_OFFICECHAIRS"
        ]
      }
    },

    {
      "comment": "product-overview",
      "commandId": "visit-uri",
      "parameters": {
        "contextId": "product-overview",
        "uri": "https://www.{template.site}/collections/chefsessel"
      }
    },
    {
      "comment": "product-ids",
      "commandId": "get-values-by-regex",
      "parameters": {
        "contextId": "product-ids",
        "sourceContextId": "product-overview",
        "groupIndex": 1,
        "regex": "sku\":\"(.*?)\"",
        "unique": true
      }
    },
    {
      "comment": "observe scraped information",
      "commandId": "observe-information",
      "parameters": {
        "informationIds": [
          "locale",
          "navigation-path",
          "product-ids",
          "site",
          "usecase"
        ]
      }
    },

    {
      "comment": "product-overview",
      "commandId": "visit-uri",
      "parameters": {
        "contextId": "product-overview",
        "overwrite": true,
        "uri": "https://www.{template.site}/collections/drehstuehle-mit-netzbespannung"
      }
    },
    {
      "comment": "product-ids",
      "commandId": "get-values-by-regex",
      "parameters": {
        "contextId": "product-ids",
        "sourceContextId": "product-overview",
        "groupIndex": 1,
        "overwrite": true,
        "regex": "sku\":\"(.*?)\"",
        "unique": true
      }
    },
    {
      "comment": "observe scraped information",
      "commandId": "observe-information",
      "parameters": {
        "informationIds": [
          "locale",
          "navigation-path",
          "product-ids",
          "site",
          "usecase"
        ]
      }
    }
  ]
}
