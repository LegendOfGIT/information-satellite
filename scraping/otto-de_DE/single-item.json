    {
      "comment": "weight in grams",
      "commandId": "get-value-by-regex",
      "parameters": {
        "contextId": "weightInG",
        "sourceContextId": "single-product-view",
        "groupIndex": 1,
        "regex": [
          "Gewicht:.*?ca.(.*?)g<",
          "Gewicht:(.*?)g<",
          ">Gewicht.*?<td>(.*?) g<"
        ]
      }
    },
    {
      "comment": "weight in grams to float",
      "commandId": "get-float-from-value",
      "parameters": {
        "contextId": "weightInG",
        "sourceContextId": "weightInG"
      }
    },

    {
      "comment": "width-in-mm",
      "commandId": "get-value-by-regex",
