{
  "site": "otto.de",

  "itemId": "{template.site}-{request.query.itemId}",

  "scraping": [
    {
      "comment": "site",
      "commandId": "set-context-value",
      "parameters": {
        "contextId": "site",
        "value": "otto-de_DE"
      }
    },
    {
      "comment": "link",
      "commandId": "set-context-value",
