{
  "site": "quelle.de",

  "scraping": [
    {
      "comment": "site",
      "commandId": "set-context-value",
      "parameters": {
        "contextId": "site",
        "value": "quelle-de_DE"
      }
    },
    {
      "comment": "locale",
      "commandId": "set-context-value",
      "parameters": {
        "contextId": "locale",
        "value": "de-DE"
      }
    },
    {
      "comment": "usecase",
      "commandId": "set-context-value",
      "parameters": {
        "contextId": "usecase",
        "value": "single-item"
      }
    },
    {
      "comment": "navigation-path",
      "commandId": "set-context-value",
      "parameters": {
        "contextId": "navigation-path",
        "value": [
          "ELECTRONICS_AND_COMPUTERS",
          "ELECTRONICS_AND_COMPUTERS_LARGE_APPLIANCES",
          "ELECTRONICS_AND_COMPUTERS_LARGE_APPLIANCES_WASHING_MACHINES"
        ]
      }
    },

    {
      "comment": "product-overview",
      "commandId": "visit-uri",
      "parameters": {
        "contextId": "product-overview",
        "uri": "https://www.{template.site}/haushaltsgeraete/waschen-trocknen/waschmaschinen/?o=novelty"
      }
    },
    {
      "comment": "product-ids",
      "commandId": "get-values-by-css-queries",
      "parameters": {
        "contextId": "product-ids",
        "css-queries": [ ".MuiBox-root a[data-testid='link']"],
        "attribute-id": "href",
        "overwrite": true,
        "sourceContextId": "product-overview"
      }
    },
    {
      "comment": "product-ids",
      "commandId": "clone-items-and-modify-clones",
      "parameters": {
        "contextId": "product-ids",
        "unique": true,
        "replacements": {
          ".*/": "",
          "\\?.*": ""
        },
        "sourceContextId": "product-ids"
      }
    },

    {
      "comment": "observe scraped information",
      "commandId": "observe-information",
      "parameters": {
        "informationIds": [
          "locale",
          "navigation-path",
          "product-ids",
          "site",
          "usecase"
        ]
      }
    },

    {
      "comment": "product-overview",
      "commandId": "visit-uri",
      "parameters": {
        "contextId": "product-overview",
        "overwrite": true,
        "uri": "https://www.{template.site}/haushaltsgeraete/waschen-trocknen/waschmaschinen/"
      }
    },
    {
      "comment": "product-ids",
      "commandId": "get-values-by-css-queries",
      "parameters": {
        "contextId": "product-ids",
        "css-queries": [ ".MuiBox-root a[data-testid='link']"],
        "attribute-id": "href",
        "overwrite": true,
        "sourceContextId": "product-overview"
      }
    },
    {
      "comment": "product-ids",
      "commandId": "clone-items-and-modify-clones",
      "parameters": {
        "contextId": "product-ids",
        "unique": true,
        "replacements": {
          ".*/": "",
          "\\?.*": ""
        },
        "sourceContextId": "product-ids"
      }
    },

    {
      "comment": "observe scraped information",
      "commandId": "observe-information",
      "parameters": {
        "informationIds": [
          "locale",
          "navigation-path",
          "product-ids",
          "site",
          "usecase"
        ]
      }
    },

    {
      "comment": "random-page-number",
      "commandId": "generate-random-number",
      "parameters": {
        "contextId": "random-page-number",
        "minimum": 2,
        "maximum": 3
      }
    },
    {
      "comment": "product-overview",
      "commandId": "visit-uri",
      "parameters": {
        "contextId": "product-overview",
        "overwrite": true,
        "uri": "https://www.{template.site}/haushaltsgeraete/waschen-trocknen/waschmaschinen/?Page=P{random-page-number}"
      }
    },
    {
      "comment": "product-ids",
      "commandId": "get-values-by-css-queries",
      "parameters": {
        "contextId": "product-ids",
        "css-queries": [ ".MuiBox-root a[data-testid='link']"],
        "attribute-id": "href",
        "overwrite": true,
        "sourceContextId": "product-overview"
      }
    },
    {
      "comment": "product-ids",
      "commandId": "clone-items-and-modify-clones",
      "parameters": {
        "contextId": "product-ids",
        "unique": true,
        "replacements": {
          ".*/": "",
          "\\?.*": ""
        },
        "sourceContextId": "product-ids"
      }
    },

    {
      "comment": "observe scraped information",
      "commandId": "observe-information",
      "parameters": {
        "informationIds": [
          "locale",
          "navigation-path",
          "product-ids",
          "site",
          "usecase"
        ]
      }
    }
  ]
}
