{
  "site": "gewuerzland.com",

  "scraping": [
    {
      "comment": "site",
      "commandId": "set-context-value",
      "parameters": {
        "contextId": "site",
        "value": "gewuerzland-de_DE"
      }
    },
    {
      "comment": "locale",
      "commandId": "set-context-value",
      "parameters": {
        "contextId": "locale",
        "value": "de-DE"
      }
    },
    {
      "comment": "usecase",
      "commandId": "set-context-value",
      "parameters": {
        "contextId": "usecase",
        "value": "single-item"
      }
    },
    {
      "comment": "navigation-path",
      "commandId": "set-context-value",
      "parameters": {
        "contextId": "navigation-path",
        "value": [
          "GROCERIES",
          "GROCERIES_FOOD",
          "GROCERIES_FOOD_SPICES"
        ]
      }
    },

    {
      "comment": "product-overview",
      "commandId": "visit-uri",
      "parameters": {
        "contextId": "product-overview",
        "uri": "https://{template.site}/Gewuerze"
      }
    },
    {
      "comment": "product-ids",
      "commandId": "get-values-by-css-queries",
      "parameters": {
        "contextId": "product-ids",
        "css-queries": [ ".product-image-wrapper a"],
        "attribute-id": "href",
        "sourceContextId": "product-overview"
      }
    },
    {
      "comment": "product-ids",
      "commandId": "clone-items-and-modify-clones",
      "parameters": {
        "contextId": "product-ids",
        "replacements": {
          ".*/": ""
        },
        "sourceContextId": "product-ids"
      }
    },
    {
      "comment": "observe scraped information",
      "commandId": "observe-information",
      "parameters": {
        "informationIds": [
          "locale",
          "navigation-path",
          "product-ids",
          "site",
          "usecase"
        ]
      }
    },

    {
      "comment": "random-page-number",
      "commandId": "generate-random-number",
      "parameters": {
        "contextId": "random-page-number",
        "minimum": 2,
        "maximum": 3
      }
    },
    {
      "comment": "product-overview",
      "commandId": "visit-uri",
      "parameters": {
        "contextId": "product-overview",
        "overwrite": true,
        "uri": "https://{template.site}/Gewuerze?p={random-page-number}"
      }
    },
    {
      "comment": "product-ids",
      "commandId": "get-values-by-css-queries",
      "parameters": {
        "contextId": "product-ids",
        "css-queries": [ ".product-image-wrapper a"],
        "attribute-id": "href",
        "overwriteValues": true,
        "sourceContextId": "product-overview"
      }
    },
    {
      "comment": "product-ids",
      "commandId": "clone-items-and-modify-clones",
      "parameters": {
        "contextId": "product-ids",
        "replacements": {
          ".*/": ""
        },
        "sourceContextId": "product-ids"
      }
    },
    {
