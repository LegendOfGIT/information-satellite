{
  "site": "dunleath.com",

  "scraping": [
    {
      "comment": "site",
      "commandId": "set-context-value",
      "parameters": {
        "contextId": "site",
        "value": "dunleath-de_DE"
      }
    },
    {
      "comment": "locale",
      "commandId": "set-context-value",
      "parameters": {
        "contextId": "locale",
        "value": "de-DE"
      }
    },
    {
      "comment": "usecase",
      "commandId": "set-context-value",
      "parameters": {
        "contextId": "usecase",
        "value": "single-item"
      }
    },
    {
      "comment": "navigation-path",
      "commandId": "set-context-value",
      "parameters": {
        "contextId": "navigation-path",
        "value": [
          "BEAUTY_CARE",
          "BEAUTY_CARE_FRAGRANCES",
          "BEAUTY_CARE_FRAGRANCES_WOMEN"
        ]
      }
    },

    {
      "comment": "product-overview",
      "commandId": "visit-uri",
      "parameters": {
        "contextId": "product-overview",
        "overwrite": true,
        "uri": "https://{template.site}/de/collections/perfumes-for-women?sort_by=created-descending"
      }
    },
    {
      "comment": "product ids",
      "commandId": "get-values-by-regex",
      "parameters": {
        "contextId": "product-ids",
        "sourceContextId": "product-overview",
        "groupIndex": 1,
        "regex": "\"sku\":\"(.*?)\""
      }
    },
    {
      "comment": "observe scraped information",
      "commandId": "observe-information",
      "parameters": {
        "informationIds": [
          "locale",
          "navigation-path",
          "product-ids",
          "site",
          "usecase"
        ]
      }
    },

    {
      "comment": "product-overview",
      "commandId": "visit-uri",
      "parameters": {
        "contextId": "product-overview",
        "overwrite": true,
        "uri": "https://{template.site}/de/collections/perfumes-for-women?sort_by=best-selling"
      }
    },
    {
      "comment": "product ids",
      "commandId": "get-values-by-regex",
      "parameters": {
        "contextId": "product-ids",
        "sourceContextId": "product-overview",
        "groupIndex": 1,
        "overwriteValues": true,
        "regex": "\"sku\":\"(.*?)\""
      }
    },
    {
      "comment": "observe scraped information",
      "commandId": "observe-information",
      "parameters": {
        "informationIds": [
          "locale",
          "navigation-path",
          "product-ids",
          "site",
          "usecase"
        ]
      }
    },

    {
      "comment": "random-page-number",
      "commandId": "generate-random-number",
      "parameters": {
        "contextId": "random-page-number",
        "minimum": 2,
        "maximum": 80
      }
    },
    {
      "comment": "product-overview",
      "commandId": "visit-uri",
      "parameters": {
        "contextId": "product-overview",
        "overwrite": true,
        "uri": "https://{template.site}/de/collections/perfumes-for-women?sort_by=best-selling&page={random-page-number}"
      }
    },
    {
      "comment": "product ids",
      "commandId": "get-values-by-regex",
      "parameters": {
        "contextId": "product-ids",
        "sourceContextId": "product-overview",
        "groupIndex": 1,
        "overwriteValues": true,
        "regex": "\"sku\":\"(.*?)\""
      }
    },
    {
      "comment": "observe scraped information",
      "commandId": "observe-information",
      "parameters": {
        "informationIds": [
          "locale",
          "navigation-path",
          "product-ids",
          "site",
          "usecase"
        ]
      }
    }
  ]
}
